<operator refresh="1" initial="0" firstpage="0" prepage="0" nextpage="0">
<op text="move"><assign name="$this.wip_move_transaction.issued" expr=""/>
	<DbDetails><Where> AND routing_sequence='%{wip_move_transaction_vLst[routing_sequence]}' and routing_seq_num='%{wip_move_transaction_vLst[routing_seq_num]}' and wip_wo_header_id='%{wip_move_transaction_vLst[wip_wo_header_id]}'</Where></DbDetails><submit next="$this.wip_move_transaction.issued" namelist="DbDetails"/>
	<DbDetails><Where>wip_wo_header_id='%{wip_move_transaction_vLst[wip_wo_header_id]}' and '%{wip_move_transaction_vLst[wo_status]}'=370</Where></DbDetails><submit next="$this.wip_move_transaction.wip_wo_header_id" namelist="DbDetails"/><DbDetails><Where>routing_sequence='%{wip_move_transaction_vLst[routing_sequence]}' and routing_seq_num='%{wip_move_transaction_vLst[routing_seq_num]}' and wip_wo_header_id='%{wip_move_transaction_vLst[wip_wo_header_id]}'</Where></DbDetails><submit next="$this.wip_move_transaction.from_routing_sequence" namelist="DbDetails"/>
	<DbDetails><queryFormat>SELECT MIN(routing_sequence) routing_sequence,routing_seq_num FROM mes_trace.wip_wo_routing_line WHERE (routing_sequence BETWEEN '%{wip_move_transaction_vLst[routing_sequence]}'+1 AND '%{wip_move_transaction_vLst[lastSeq]}') AND (routing_seq_num =0 OR routing_seq_num='%{wip_move_transaction_vLst[routing_seq_num]}') AND wip_wo_header_id='%{wip_move_transaction_vLst[wip_wo_header_id]}' GROUP BY routing_seq_num</queryFormat></DbDetails><submit next="$this.wip_move_transaction.to_routing_sequence" namelist="DbDetails"/><assign name="$this.wip_move_transaction.from_operation_step" expr="380"/><assign name="$this.wip_move_transaction.to_operation_step" expr="382"/><assign name="$this.wip_move_transaction.move_qty" expr="%{wip_move_transaction_vLst[running_quantity]}"/><assign name="$this.wip_move_transaction.move_quantity" expr="%{wip_move_transaction_vLst[running_quantity]}"/></op>
</operator>
