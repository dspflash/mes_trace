<GroupBox Name="GBDEDITitem" text="Search" toggle="1"><Ctrls>
<GroupBox Name="GB_item_number" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_item_number" Size="120,20" Text="item_number" Align="Right"/>
<TextBox Name="item_number" Size="110,20" Text="" Weight="110" ToolTip="ttp_item_number"></TextBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_item_name" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_item_name" Size="120,20" Text="item_name" Align="Right"/>
<TextBox Name="item_name" Size="110,20" Text="" Weight="110" ToolTip="ttp_item_name"></TextBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_item_specification" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_item_specification" Size="120,20" Text="item_specification" Align="Right"/>
<TextBox Name="item_specification" Size="110,20" Text="" Weight="110" ToolTip="ttp_item_specification"></TextBox>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox Name="GB_item_category" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_item_category" Size="120,20" Text="item_category" Align="Right" onload="init"/>
<ComboBox Name="item_category" Size="90,100" Text="" Weight="90"  ToolTip="ttp_item_category" onload="init">
	<DbDetails>
<Index>category_id</Index>
<DbTable>mes_trace.category</DbTable>
<Data>category,category_id</Data>
<Where></Where>
<queryFormat></queryFormat></DbDetails></ComboBox>
<MagicCtrl Name="refreshitem_category" Link="tip:刷新" images=",data/refresh.png" button="type:0" Size="20,20"><OnClick><submit next="item_category"/></OnClick></MagicCtrl>\r\n</Ctrls></GroupBox>
<GroupBox Name="GB_make_buy" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_make_buy" Size="120,20" Text="make_buy" Align="Right" selectedIndex="1"/>
<ComboBox Name="make_buy" Size="110,100" Text="" Weight="110" Items="Make|Buy" ToolTip="ttp_make_buy" selectedIndex="1"></ComboBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_bc_label" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_bc_label" Size="120,20" Text="bc_label" Align="Right"/>
<TextBox Name="bc_label" Size="110,20" Text="" Weight="110" ToolTip="ttp_bc_label"></TextBox>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox Name="GB_item_status" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_item_status" Size="120,20" Text="item_status" Align="Right" onload="init" selectedIndex="0"/>
<ComboBox Name="item_status" Size="110,100" Text="" Weight="110"  ToolTip="ttp_item_status" onload="init" selectedIndex="0">
	<DbDetails>
<Index>option_line_id</Index>
<DbTable>mes_trace.option_line</DbTable>
<Data>option_line_value,option_line_id</Data>
<Where>option_header_id=123</Where>
<queryFormat></queryFormat></DbDetails></ComboBox>
</Ctrls></GroupBox>
</Ctrls></GroupBox>