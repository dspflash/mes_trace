	<DbDetails>
<Index>item_id_m</Index>
<DbTable>mes_trace.item</DbTable>
<Data>item_number,item_id_m</Data>
<Where>item_id_m='%{wip_wo_header_id[item_id_m]}'</Where>
<queryFormat/></DbDetails></TextBox>
</Ctrls></GroupBox>
