<?xml version="1.0" encoding="UTF-8"?><biztoolcfg qryfmtOpCreate="" qryfmtOpUpdate="" loadedClick="0" tableNm="bom_line" referLst="0" onLoadInit="0" tableTxt="bom_line" LstTP="Lst" lines2Link="" jointbls="1" header2Link="bom_header_id" exdata4select=""><DataLstExtElmt><![CDATA[<DbDetails>
