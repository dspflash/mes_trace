</Ctrls></GroupBox>

<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_item_name"><Ctrls>
<Label Align="Right" Text="item_name" Name="lb_item_name" Size="120,20"/>
<TextBox Weight="170" Text="" ToolTip="ttp_item_name" Name="item_name" Size="170,20">
	<DbDetails fcsm="1">
<DbTable>mes_trace.item i left join mes_trace.category cg on i.item_category=cg.category_id left join mes_trace.ar_customer c on i.default_buyer=c.ar_customer_id</DbTable>
<Index>item_id_m</Index>
<Data>item_name,customer_name default_buyer,category item_tp,item_number,i.long_description,item_id_m</Data>
<Where>item_type=99</Where>
<queryFormat/></DbDetails>
</TextBox>
</Ctrls></GroupBox>

<BR AutoHeight="1"/><GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_item_tp"><Ctrls>
