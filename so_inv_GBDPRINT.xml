<GroupBox Name="GBDEDITwip_wo_header"><Ctrls>
<GroupBox cols="xs12 sm6" Multiline="0" Name="GB_org_name"><Ctrls>
<Label Align="Right" Text="" Name="lb_org_name" Size="120,20" Weight="20" />
