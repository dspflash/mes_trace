</Ctrls></GroupBox>

<BR AutoHeight="1"/><GroupBox cols="xs12 sm6" Multiline="0" Name="GB_long_description"><Ctrls>
<Label Align="Right" Text="long_description" Name="lb_long_description" Size="120,20"/>
<TextBox Weight="170" Text="" ToolTip="ttp_long_description" Name="long_description" Size="170,20"/>
<TextBox Weight="110" Text="新模" ToolTip="ttp_model" Name="model" Size="170,20"/>
</Ctrls></GroupBox>

<BR AutoHeight="1"/><GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_default_buyer"><Ctrls>
<Label Align="Right" Text="default_buyer" Name="lb_default_buyer" Size="120,20"/>
